{
    "name": "unitconversion/unit-conversion",
    "description": "This is my package unit-conversion",
    "keywords": [
        "UnitConversion",
        "unit-conversion"
    ],
    "homepage": "https://github.com/unitconversion/unit-conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Suraj Bista",
            "email": "sbista63@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "UnitConversion\\UnitConversion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitConversion\\UnitConversion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
